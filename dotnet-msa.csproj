<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <DebugType>portable</DebugType>
    <AssemblyName>dotnet-msa</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>dotnet-msa</PackageId>
    <RuntimeIdentifiers>rhel.7.3-x64</RuntimeIdentifiers>
    <RuntimeFrameworkVersion>2.0.0-*</RuntimeFrameworkVersion>
    <PackageTargetFallback>$(PackageTargetFallback);portable-net45+wp80+win8+wpa81+dnxcore50</PackageTargetFallback>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Server.Kestrel" Version="1.1.0" />
  </ItemGroup>

</Project>
